<h1>My Profile</h1>
<p>Email: <%= current_user.email %></p>
<p><%= link_to "Edit Profile", edit_user_registration_path %></p>

<p style="color: green"><%= notice %></p>

<h3>Events created by you:</h3>

<p><%= link_to "New event", new_event_path %></p>

<div id="events">
  <% @created_events.each do |event| %>
    <%= render event %>
    <p>
      <%= link_to "Show this event", event %>
    </p>
  <% end %>
</div>